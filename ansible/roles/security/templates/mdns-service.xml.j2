<?xml version="1.0" standalone='no'?>
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <name replace-wildcards="yes">{{ service_hostname }} on %h</name>
  <service>
    <type>_http._tcp</type>
    <port>443</port>
    <host-name>{{ service_hostname }}.local</host-name>
    <txt-record>path=/</txt-record>
  </service>
</service-group>
