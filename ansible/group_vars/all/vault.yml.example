# Ansible Vault - Encrypted Secrets
# Create this file with: ansible-vault create group_vars/all/vault.yml

# k3s cluster token - AUTO-GENERATED by Ansible for single-node setup!
# No need to set vault_k3s_token - Ansible creates it automatically

# Tailscale auth key (get from https://login.tailscale.com/admin/settings/keys)
vault_tailscale_auth_key: "tskey-auth-your-tailscale-auth-key-here"
